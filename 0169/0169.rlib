impl Solution {
    pub fn majority_element(nums: Vec<i32>) -> i32 {
        let mut a = 0;
        let mut c = 0;

        for n in nums {
            if c == 0 {
                a = n;
                c += 1;
            }else if a == n {
                c += 1;
            }else {
                c -= 1;
            }
        }
        return a;
    }
}